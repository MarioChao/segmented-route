{
  "name": "route-segment",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Modules": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,

        "SegmentedRoute": {
          "$path": "src/ReplicatedStorage/SegmentedRoute"
        }
      }
    },

    "Workspace": {
      "$path": "src/Workspace",
      "$ignoreUnknownInstances": true
    }
  },
  "globIgnorePaths": ["**/wally.toml"]
}